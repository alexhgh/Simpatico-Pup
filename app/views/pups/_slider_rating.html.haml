%div{:class =>"price-slider row"}
    %div{:class=>"col-sm-5"}
        %h4{:class => "great"}=label :pup, var, title
        %br
        %span #{description}
    %div{:class=>"col-sm-2"}
        %br
        %br
        %br
        %h5 #{des1}
    %div{:class=>"col-sm-3 paw"}
        %br
        %br
        %br
        %div{:class => "paw"}
            %div{:class => "pull-left"}
                %i{:class =>"fa fa-paw fa-2x", :id => "#{id}-first"}
            %div{:class => "pull-left"}
                %i{:class =>"fa fa-paw fa-2x", :id => "#{id}-second"}
            %div{:class => "pull-left"}
                %i{:class => "fa fa-paw fa-2x", :id => "#{id}-third"}
            %div{:class => "pull-left"}
                %i{:class =>"fa fa-paw fa-2x", :id => "#{id}-fourth"}
            %div{:class => "pull-left"}
                %i{:class => "fa fa-paw fa-2x", :id => "#{id}-fifth"}
            %div{:class => "pull-right"}
                %i{:class => "fa fa-paw fa-2x", :id => "#{id}-sixth"}
            %div{:class=>"text-left"}
                = text_field :pup, var, :class => "read", :id => id, :value => "0", :readonly => true
    %div{:class=>"col-sm-2"}
        %br
        %br
        %br
        %h5 #{des5}
    
                        